    <!-- <div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column"> -->
    <div class="container my-4">
        <h2>Contact Us</h2>
        <form action="/contact1" method="POST" class="form-container">
            <div class="form-group">
              <label for="email">Email address</label>
              <input type="email" name="email" class="form-control" id="email" placeholder="name@example.com">
            </div>
            <div class="form-group">
              <label for="queryselect">Select your query</label>
              <select class="form-control" name ="queryselect" id="queryselect">
                <option>Rent</option>
                <option>PG</option>
                <option>Account related</option>
                <option>Other</option>
              </select>
            </div>
            <div class="form-group">
              <label for="concern_msg">Elaborate your concern</label>
              <textarea class="form-control" id="concern_msg" name="concern_msg" rows="3"></textarea>
            </div>
            
            <button class="btn btn-primary my-2 my-sm-0" type="submit">Submit</button>

          </form>
       
    </div>

    <footer class="container">
